{
  "name": "whittle-school",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "GATSBY_REGION=\"US\" gatsby build",
    "build-china": "GATSBY_REGION=\"CHINA\" gatsby build",
    "build-ci": "if test \"$CONTEXT\" = \"deploy-preview\"; then yarn lint-js && yarn lint-css && yarn test; fi && yarn build",
    "build-ci-china": "if test \"$CONTEXT\" = \"deploy-preview\"; then yarn lint-js && yarn lint-css && yarn test; fi && yarn build-china",
    "develop": "GATSBY_REGION=\"US\" gatsby develop",
    "develop-china": "GATSBY_REGION=\"CHINA\" gatsby develop",
    "lint-js": "eslint '**/*.js' --ignore-path .eslintignore --max-warnings 0",
    "lint-css": "stylelint '**/*.css'",
    "start": "gatsby serve",
    "copy-cn-to-us": "babel scripts -s -D -d bin --presets es2015,stage-0 && node bin/cn-dev-to-us-dev.js",
    "deploy-qa": "babel scripts -s -D -d bin --presets es2015,stage-0 && node bin/deploy-qa.js",
    "deploy-prod": "babel scripts -s -D -d bin --presets es2015,stage-0 && node bin/deploy-prod.js",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "dependencies": {
    "@vimeo/player": "git://github.com/vimeo/player.js#v2.2.1",
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.5",
    "detect-touch-events": "^2.0.1",
    "eslint": "^4.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "file-system": "^2.2.2",
    "gatsby": "^1.9.150",
    "gatsby-link": "^1.6.30",
    "gatsby-plugin-favicon": "^2.1.1",
    "gatsby-plugin-google-analytics": "^1.0.15",
    "gatsby-plugin-react-helmet": "^1.0.5",
    "gatsby-plugin-react-next": "^1.0.7",
    "gatsby-source-contentful": "^1.3.31",
    "gatsby-transformer-remark": "^1.7.16",
    "react": "^16.2.0",
    "react-cookie": "^2.1.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-image-gallery": "git://github.com/sarahatwork/react-image-gallery#a11y-fixes",
    "react-markdown": "^2.5.0",
    "react-plx": "^1.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "request": "^2.83.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "contentful-export": "^4.7.8",
    "contentful-import": "^4.5.7",
    "contentful-management": "^4.1.2",
    "dotenv": "^4.0.0",
    "eslint-config-airbnb": "^16.1.0",
    "jest": "^22.0.1",
    "moment-timezone": "^0.5.14",
    "postcss": "6.0.13",
    "postcss-browser-reporter": "0.5.0",
    "postcss-calc": "6.0.1",
    "postcss-conditionals": "2.1.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-discard-duplicates": "2.1.0",
    "postcss-extend": "^1.0.5",
    "postcss-for": "2.1.1",
    "postcss-import": "11.0.0",
    "postcss-load-config": "1.2.0",
    "postcss-loader": "2.0.8",
    "postcss-mixins": "6.2.0",
    "postcss-nested": "^2.1.2",
    "postcss-pxtorem": "^4.0.1",
    "postcss-reporter": "5.0.0",
    "postcss-simple-vars": "4.1.0",
    "simple-git": "^1.82.0",
    "stylelint": "~8.2.0",
    "stylelint-order": "^0.7.0"
  }
}
